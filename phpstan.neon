includes:
  - ./vendor/larastan/larastan/extension.neon

parameters:
  level: 9
  paths:
    - app
  excludePaths:
    - app/Legacy/*
    - app/Nova
  tmpDir: build/phpstan
  checkOctaneCompatibility: true
  checkModelProperties: true
  checkMissingIterableValueType: false
  scanFiles:
#    - _ide_helper.php
#    - _ide_helper_models.php
