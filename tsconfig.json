{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2021",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "types": [
        "vite/client",
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"]
    }
  },
  "include": [
      "./resources/js/**/*",
  ],
  "exclude": ["node_modules", "public"]
}
