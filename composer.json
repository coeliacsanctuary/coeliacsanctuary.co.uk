{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.8.0",
    "inertiajs/inertia-laravel": "^0.6.10",
    "intervention/image": "^2.7.2",
    "jpeters8889/advanced-nova-media-library": "dev-master",
    "jpeters8889/body": "*",
    "jpeters8889/polymorphic-panel": "*",
    "laravel/framework": "^10.25.1",
    "laravel/nova": "^4.27.13",
    "laravel/sanctum": "^3.3.1",
    "laravel/tinker": "^2.8.2",
    "league/flysystem-aws-s3-v3": "^3.16.0",
    "pixelcreation/nova-field-sortable": "^3.2",
    "spatie/eloquent-sortable": "^4.0.2",
    "spatie/laravel-collection-macros": "^7.13",
    "spatie/laravel-data": "^2.2.3",
    "spatie/laravel-medialibrary": "^10.13.0",
    "spatie/laravel-sluggable": "^3.5.0",
    "spatie/schema-org": "^3.14"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9.2",
    "brianium/paratest": "^7.2.7",
    "fakerphp/faker": "^1.23.0",
    "laravel/pint": "^1.13.2",
    "laravel/sail": "^1.25.0",
    "mockery/mockery": "^1.6.6",
    "nunomaduro/collision": "^v7.3.3",
    "nunomaduro/larastan": "^2.6.4",
    "phpunit/phpunit": "^10.3.5",
    "spatie/laravel-ignition": "^2.3.0",
    "spatie/test-time": "^1.3.2",
    "worksome/request-factories": "^3.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "composer",
      "url": "https://satis.spatie.be"
    },
    {
      "type": "path",
      "url": "./../packages/advanced-nova-media-library"
    },
    {
      "type": "path",
      "url": "./nova-components/Body"
    },
    {
      "type": "path",
      "url": "./nova-components/PolymorphicPanel"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "stan": "vendor/bin/phpstan analyse",
    "pint": "vendor/bin/pint -v",
    "test": "php artisan test --parallel",
    "pre-commit": "composer run pint && composer run stan && composer run test"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "10.x-dev"
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
