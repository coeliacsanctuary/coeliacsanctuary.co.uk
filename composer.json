{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "algolia/algoliasearch-client-php": "^3.4.1",
        "algolia/scout-extended": "^3.0",
        "guzzlehttp/guzzle": "^7.8.1",
        "inertiajs/inertia-laravel": "^0.6.11",
        "intervention/image": "^2.7.2",
        "jpeters8889/address-field": "@dev",
        "jpeters8889/advanced-nova-media-library": "dev-master",
        "jpeters8889/body": "*",
        "jpeters8889/eatery-opening-times": "@dev",
        "jpeters8889/eatery-recommendation-listener": "@dev",
        "jpeters8889/polymorphic-panel": "*",
        "kirschbaum-development/nova-inline-relationship": "^2.1.1",
        "laravel/framework": "^10.41.0",
        "laravel/nova": "^4.32.11",
        "laravel/sanctum": "^3.3.3",
        "laravel/scout": "^10.8.0",
        "laravel/tinker": "^2.9.0",
        "league/flysystem-aws-s3-v3": "^3.22.0",
        "moneyphp/money": "^4.3",
        "pixelcreation/nova-field-sortable": "^3.2",
        "spatie/eloquent-sortable": "^4.1.0",
        "spatie/geocoder": "^3.15",
        "spatie/laravel-collection-macros": "^7.13.1",
        "spatie/laravel-data": "^2.2.3",
        "spatie/laravel-medialibrary": "^10.15.0",
        "spatie/laravel-sluggable": "^3.5.0",
        "spatie/mjml-php": "^1.1",
        "spatie/schema-org": "^3.23",
        "stripe/stripe-php": "^13.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9.2",
        "barryvdh/laravel-ide-helper": "^2.13",
        "brianium/paratest": "^7.3.1",
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.8.1",
        "laravel/pint": "^1.13.9",
        "laravel/sail": "^1.27.1",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^v7.3.3",
        "phpunit/phpunit": "^10.5.7",
        "spatie/laravel-ignition": "^2.4.1",
        "spatie/test-time": "^1.3.2",
        "sti3bas/laravel-scout-array-driver": "^3.5",
        "worksome/request-factories": "^3.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        },
        {
            "type": "path",
            "url": "./../packages/advanced-nova-media-library"
        },
        {
            "type": "path",
            "url": "./nova-components/Body"
        },
        {
            "type": "path",
            "url": "./nova-components/PolymorphicPanel"
        },
        {
            "type": "path",
            "url": "./nova-components/AddressField"
        },
        {
            "type": "path",
            "url": "./nova-components/AddressField"
        },
        {
            "type": "path",
            "url": "./nova-components/EateryOpeningTimes"
        },
        {
            "type": "path",
            "url": "./nova-components/EateryRecommendationListener"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "stan": "vendor/bin/phpstan analyse",
        "pint": "vendor/bin/pint -vvv",
        "test": "php artisan test --parallel",
        "pre-commit": "composer run pint && composer run stan && composer run test"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
