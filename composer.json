{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "inertiajs/inertia-laravel": "^0.6.9",
        "laravel/framework": "^10.5.1",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8.1",
        "league/flysystem-aws-s3-v3": "^3.12.2",
        "spatie/laravel-data": "^2.2.3",
        "spatie/laravel-medialibrary": "^10.7.15",
        "spatie/laravel-sluggable": "^3.4.2",
        "spatie/schema-org": "^3.14"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "brianium/paratest": "^7.1.2",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.7",
        "laravel/sail": "^1.21.3",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^v7.3.3",
        "nunomaduro/larastan": "^2.5.1",
        "phpunit/phpunit": "^10.0.19",
        "spatie/laravel-ignition": "^2.0",
        "worksome/request-factories": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": "vendor/bin/phpstan analyse",
        "pint": "vendor/bin/pint -v",
        "test": "php artisan test --parallel"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
